extends layout-front

block content-front
  #main-slide.carousel.slide(data-ride='carousel')
    ol.carousel-indicators
      template(v-for="item, iter in sliderResults")
        template(v-if='iter == 0')
          li.active(data-target='#main-slide', v-bind:data-slide-to= 'iter')
        template(v-else)
          li(data-target='#main-slide', v-bind:data-slide-to= 'iter')
    .carousel-inner
      template(v-for="item, iter in sliderResults")
        template(v-if='iter == 0')
          .carousel-item.active
            img.d-block.w-100(v-bind:src="'assets/images/slider/'+item.file", alt='First slide')
            .carousel-caption.d-md-block
              h1.animated.wow.fadeInDown.hero-heading(data-wow-delay='.4s') {{ item.title }}
              h5.animated.fadeInUp.wow.hero-sub-heading(data-wow-delay='.6s') {{ item.description }}
        template(v-else)
          .carousel-item
            img.d-block.w-100(v-bind:src="'assets/images/slider/'+item.file", alt='First slide')
            .carousel-caption.d-md-block
              h1.animated.wow.fadeInDown.hero-heading(data-wow-delay='.4s') {{ item.title }}
              h5.animated.fadeInUp.wow.hero-sub-heading(data-wow-delay='.6s') {{ item.description }}

    a.carousel-control-prev(href='#main-slide', role='button', data-slide='prev')
      span.carousel-control(aria-hidden='true')
        i.mdi.mdi-arrow-left(data-ripple-color='#F0F0F0')
      span.sr-only Previous
    a.carousel-control-next(href='#main-slide', role='button', data-slide='next')
      span.carousel-control(aria-hidden='true')
        i.mdi.mdi-arrow-right(data-ripple-color='#F0F0F0')
      span.sr-only Next
