extends layout-front

block content-front
  section.portfolio-details-section.section-padding
  .container
    .row
      .col-md-12.Material-title-section.wow.animated.fadeInLeft(data-wow-delay='.2s')
        h1.section-title All Promo's
    .row
      .col-md-8.portfolio-details-iamge.wow.animated.fadeInUp(data-wow-delay='.6s')
        img.img-fluid(src='assets/images/portfolio/portfolio-details.png', alt='')
      .col-md-4.portfolio-details.wow.animated.fadeInUp(data-wow-delay='.6s')
        .portfolio-category
          a(href='#')
            i.material-icons.mdi.mdi-bookmark-outline
            |  Wordpress Theme
        h2 CONBIZ
        p
          | Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco.
